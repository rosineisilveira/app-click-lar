        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rosinei2.appclicklar"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5ffcdf64-e23e-46e6-b2dc-a62a03a65c63"
      }
    },
    "plugins": [
      "expo-font"
    ]
